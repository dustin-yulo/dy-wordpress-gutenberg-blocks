{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "dy-wordpress-gutenberg-blocks/button-with-icon",
	"version": "1.0.0",
	"title": "Button with Icon",
	"category": "dy-wordpress-gutenberg-blocks",
	"description": "Use free icons from Font Awesome to display with your buttons",
	"supports": {
		"html": false
	},
	"textdomain": "dy-wordpress-gutenberg-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"selectedFontAwesomeIcon": {
			"type": "object",
			"default": {}
		},
		"fontAwesomeIconPosition": {
			"type": "string",
			"default": "left"
		},
		"buttonText": {
			"type": "string",
			"default": ""
		},
		"buttonLink": {
			"type": "string"
		},
		"buttonLinkTarget": {
			"type": "string"
		},
		"buttonLinkRel": {
			"type": "string"
		},
		"buttonTextAlignment": {
			"type": "string"
		},
		"buttonTextColor": {
			"type": "string"
		},
		"buttonBackgroundColor": {
			"type": "string"
		},
		"buttonBorderColor": {
			"type": "string"
		},
		"buttonBorderSettings": {
			"type": "object"
		},
		"buttonBorderRadius": {
			"type": "object"
		},
		"buttonWidth": {
			"type": "string"
		}
	}
}
