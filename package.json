{
	"name": "dy-wordpress-gutenberg-blocks",
	"version": "1.0.0",
	"description": "Custom WordPress Gutenberg blocks that you just might need. Simple, lightweight, free!",
	"author": "Dustin Yulo",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"precommit": "npm run format && npm run lint:css && npm run lint:js && npm run build"
	},
	"stylelint": {
		"extends": "@wordpress/stylelint-config/scss",
		"rules": {
			"selector-class-pattern": null
		}
	},
	"eslintConfig": {
		"extends": [
			"plugin:@wordpress/eslint-plugin/recommended"
		],
		"rules": {
			"@wordpress/no-unsafe-wp-apis": 1
		}
	},
	"devDependencies": {
		"@wordpress/eslint-plugin": "^14.3.0",
		"@wordpress/scripts": "^25.4.0",
		"@wordpress/stylelint-config": "^21.13.0"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.3.0",
		"@fortawesome/free-brands-svg-icons": "^6.3.0",
		"@fortawesome/free-regular-svg-icons": "^6.3.0",
		"@fortawesome/free-solid-svg-icons": "^6.3.0",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@wordpress/block-editor": "^11.6.0",
		"@wordpress/components": "^23.7.0",
		"react-window": "^1.8.8"
	}
}
